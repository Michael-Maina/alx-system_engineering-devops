#!/usr/bin/env bash
# This script that loops 20 times and displays the string "bad luck from China" for the 4th iteration, "bad luck from Japan" for the 9th iteration, "bad luck from Italy" for the 17th iteration and numbers for all others.
i=1
while [ $i -le 20 ]; do
    case $i in
	4)
	    echo "bad luck from China"
	    ;;
	8)
	    echo "bad luck from Japan"
	    ;;
	17)
	    echo "bad luck from Italy"
	    ;;
	*)
	    echo $i
    esac
    i=$(( i + 1 ))
done
